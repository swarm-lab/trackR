citHeader("To cite trackR in publications use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

citEntry(
  entry    = "Manual",
  title    = "{trackR} - Multi-object tracking with R",
  author   = "Simon Garnier",
  journal  = "",
  year     = year,
  volume   = "",
  number   = "",
  pages    = "",
  note     = note,
  url      = "https://swarm-lab.github.io/trackR/",
  textVersion = paste0(
    "Simon Garnier (", year, "). trackR - Multi-object tracking with R. ", note, "."
  )
)
